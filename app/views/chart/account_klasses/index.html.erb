<p id="notice"><%= notice %></p>

<h1>Chart Account Klasses</h1>

<table>
  <thead>
    <tr>
      <th>Lang key</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @chart_account_klasses.each do |chart_account_klass| %>
      <tr>
        <td><%= chart_account_klass.lang_key %></td>
        <td><%= link_to 'Show', chart_account_klass %></td>
        <td><%= link_to 'Edit', edit_chart_account_klass_path(chart_account_klass) %></td>
        <td><%= link_to 'Destroy', chart_account_klass, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Chart Account Klass', new_chart_account_klass_path %>
